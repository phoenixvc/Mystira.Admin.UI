# Azure AD (Entra ID) Configuration
# These values are required for MSAL authentication

# Azure AD Application (client) ID
VITE_AZURE_CLIENT_ID=your-client-id-here

# Azure AD Directory (tenant) ID
VITE_AZURE_TENANT_ID=your-tenant-id-here

# Redirect URI after login (defaults to window.location.origin)
VITE_AZURE_REDIRECT_URI=http://localhost:5173

# API Scope for accessing backend API
# Format: api://{api-client-id}/.default or api://{api-client-id}/access_as_user
VITE_AZURE_API_SCOPE=api://your-api-client-id/.default

# Backend API Base URL
VITE_API_BASE_URL=http://localhost:5000
